<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Open Cart Test Suite" verbose="4" thread-count="4" parallel="tests">

	<listeners>
		<listener class-name="com.qa.opencart.listeners.ExtentReportListener" />
		<listener class-name="com.qa.opencart.listeners.TestAllureListener" /> 
		<listener class-name="com.qa.opencart.listeners.AnnotationTransformer" /> 
	</listeners>

	<test name="Open Cart Login Page Test_chrome_89">
		<parameter name="browser" value="chrome"></parameter>
		<parameter name="browserVersion" value="89.0"></parameter>
		<classes>
			<class name="com.qa.opencart.test.LoginPageTest" />
		</classes>
	</test>
	
